{
	"workbench.colorTheme": "Default Dark+ Experimental",
	"explorer.confirmDelete": false,
	"editor.defaultFormatter": "esbenp.prettier-vscode",
	"editor.minimap.enabled": false,
	"files.exclude": {
		"**/.classpath": true,
		"**/.factorypath": true,
		"**/.project": true,
		"**/.settings": true,
		"**/node_modules__": true,
		"**/package-lock.json": true
	},
	"git.enableSmartCommit": true,
	"git.confirmSync": false,
	"editor.cursorSmoothCaretAnimation": "on",
	"keyboard.dispatch": "keyCode",
	"update.mode": "manual",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"explorer.confirmDragAndDrop": false,
	"editor.inlineSuggest.enabled": true,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": true,
		"markdown": true
	},
	"editor.suggestSelection": "first",
	"vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
	"java.imports.gradle.wrapper.checksums": [
		{
			"sha256": "e2b82129ab64751fd40437007bd2f7f2afb3c6e41a9198e628650b22d5824a14",
			"allowed": true
		}
	],
	"prettier.printWidth": 120,
	"editor.insertSpaces": false,
	"[rust]": {
		"editor.defaultFormatter": "matklad.rust-analyzer",
		"editor.tabSize": 4,
		"editor.detectIndentation": false,
		"editor.insertSpaces": false,
		"editor.formatOnType": true,
		"editor.formatOnSaveMode": "file"
	},
	"launch": {
		"version": "0.2.0",
		"configurations": [
			{
				"type": "pwa-node",
				"request": "attach",
				"name": "Attach to nodemon process((hint: which has ``nodemon --inpect``))",
				"processId": "${command:PickProcess}",
				"restart": true,
				"envFile": "${workspaceFolder}/.env"
			},
			{
				"type": "chrome",
				"request": "launch",
				"name": "Launch Chrome against localhost",
				"url": "http://localhost:3000",
				"webRoot": "${workspaceFolder}"
			}
		]
	},
	"security.workspace.trust.untrustedFiles": "open",
	"window.menuBarVisibility": "toggle",
	"redhat.telemetry.enabled": false,
	"editor.minimap.maxColumn": 160,
	"editor.wordWrapColumn": 120,
	"task.problemMatchers.neverPrompt": {
		"shell": true
	},
	"typescript.updateImportsOnFileMove.enabled": "always",
	"css.styleSheets": ["https://cdn.jsdelivr.net/npm/bootstrap@5.2.3/dist/css/bootstrap.min.css"],
	"javascript.preferences.jsxAttributeCompletionStyle": "none",
	"tailwindCSS.emmetCompletions": true,
	"hardhat.telemetry": true,
	"git.autofetch": true,
	"typescript.preferences.importModuleSpecifier": "relative",
	"solidity.telemetry": true,
	"[python]": {
		"editor.formatOnType": true
	},
	"liveshare.presence": true,
	"turboConsoleLog.addSemicolonInTheEnd": true,
	"turboConsoleLog.includeFileNameAndLineNum": false,
	"turboConsoleLog.insertEnclosingClass": false,
	"turboConsoleLog.insertEnclosingFunction": false,
	"turboConsoleLog.logMessageSuffix": "?",
	"turboConsoleLog.quote": "'",
	"console-ninja.featureSet": "Pro",
	"editor.formatOnSaveMode": "modifications",
	"editor.tabSize": 2,
	"terminal.integrated.env.linux": {},
	// Below settings are for slasher project's eslint auto-fix all problems on file save.
	// Format setting for `typescript`, `typescriptreact`, `javascript`, javascriptreact
	"[typescript]": {
		"editor.formatOnSave": false,
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": false,
			"source.addMissingImports": false
		},
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"[typescriptreact]": {
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true,
			"source.addMissingImports": true
		},
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},

	"[javascript]": {
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true,
			"source.addMissingImports": true
		},
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},

	"[javascriptreact]": {
		"editor.formatOnSave": true,
		"editor.codeActionsOnSave": {
			"source.fixAll.eslint": true,
			"source.addMissingImports": true
		},
		"editor.defaultFormatter": "vscode.typescript-language-features"
	},
	"gitlens.views.contributors.showAllBranches": true,
	"gitlens.views.contributors.files.layout": "list",
	"[ignore]": {
		"editor.defaultFormatter": "foxundermoon.shell-format"
		},
		"gitlens.autolinks": [
			{
				"url": "https://slasher.atlassian.net/browse/SD-<num>",
				"prefix": "SD-"
			}
		],
		"errorLens.excludeBySource": [
			"eslint(no-console)",
			"eslint(no-alert)"
		],
		"editor.linkedEditing": true,
		"git.openRepositoryInParentFolders": "always",
		"[dotenv]": {
			"editor.defaultFormatter": "foxundermoon.shell-format"
		},
		"gitlens.views.commitDetails.files.layout": "list",
		"diffEditor.ignoreTrimWhitespace": false,
		"[shellscript]": {
			"editor.defaultFormatter": "foxundermoon.shell-format"
		},
		"files.associations": {
			"config": "shellscript"
		},
		"[properties]": {
			"editor.defaultFormatter": "foxundermoon.shell-format"
		},
		"turboConsoleLog.logMessagePrefix": "",
		"eslint.options": {
			"overrideConfig": {
				"rules": {
					"no-multiple-empty-lines": ["error", { "max": 1, "maxBOF": 0, "maxEOF": 0 }],
				}
			}
		},
		"mdb.confirmRunAll": false,
		"mdb.confirmDeleteDocument": false,
		"diffEditor.renderSideBySide": false,
		"[dart]": {
			"editor.formatOnSave": true,
			"editor.formatOnType": true,
			"editor.rulers": [
				80
			],
			"editor.selectionHighlight": false,
			"editor.suggestSelection": "first",
			"editor.tabCompletion": "onlySnippets",
			"editor.wordBasedSuggestions": false
		}
}